<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog">
<changeSet id="create tables" author="redfox">
<createTable tableName="APPROVE_TYPE">
    <column name="ID" type="bigint">
        <constraints primaryKey="true" nullable="false"/>
    </column>
    <column name="VERSION" type="bigint"/>
    <column name="APPROVE_TYPE_NAME" type="varchar(255)">
        <constraints nullable="false"/>
    </column>
    <column name="DESCRIPTIONS" type="varchar(255)"/>
</createTable>
<createTable tableName="APPROVE">
    <column name="ID" type="bigint">
        <constraints primaryKey="true" nullable="false"/>
    </column>
    <column name="VERSION" type="bigint"/>
    <column name="APPROVE_TYPE_ID" type="bigint">
        <constraints nullable="false"
             foreignKeyName="FK_APPROVE_APPROVE_TYPE_ID"
             referencedTableName="APPROVE" referencedColumnNames="ID"/>
    </column>
    <column name="BEGIN_DATE" type="datetime">
        <constraints nullable="true"/>
    </column>
    <column name="END_DATE" type="datetime">
        <constraints nullable="true"/>
    </column>
    <column name="PROCESS_DEFINITION_KEY" type="varchar(64)">
        <constraints nullable="false"/>
    </column>
    <column name="PROCESS_DEFINITION_VERSION" type="varchar(64)">
        <constraints nullable="true"/>
    </column>
</createTable>

<createTable tableName="DOC_TYPE">
    <column name="ID" type="bigint">
        <constraints primaryKey="true" nullable="false"/>
    </column>
    <column name="VERSION" type="bigint"/>
    <column name="TYPE_NAME" type="varchar(255)">
        <constraints nullable="false"/>
    </column>
    <column name="APPROVE_TYPE_ID" type="bigint">
        <constraints nullable="true"
             foreignKeyName="FK_DOC_TYPE_APPROVE_TYPE_ID"
             referencedTableName="APPROVE" referencedColumnNames="ID"/>
    </column>

</createTable>
<createTable tableName="DOCUMENT">
    <column name="ID" type="bigint">
        <constraints primaryKey="true" nullable="false"/>
    </column>
    <column name="VERSION" type="bigint"/>
    <column name="DOCUMENT_NO" type="varchar(255)">
        <constraints nullable="false"/>
    </column>
    <column name="DOC_DATE" type="date">
        <constraints nullable="false"/>
    </column>
    <column name="DOCUMENT_TYPE_ID" type="bigint">
        <constraints nullable="false"
             foreignKeyName="FK_DOCUMENT_DOC_TYPE_ID"
             referencedTableName="DOCUMENT" referencedColumnNames="ID"/>
    </column>
</createTable>

</changeSet>

</databaseChangeLog>